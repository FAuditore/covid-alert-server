{
	"name": "Ruby, Go, and MySQL",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspace",

	"settings": { 
		"terminal.integrated.shell.linux": "/bin/bash"
	},

	"extensions": [
		"rebornix.Ruby"
	],

	"postCreateCommand": "export PATH=/usr/local/go/bin:$PATH && bundle install",
}